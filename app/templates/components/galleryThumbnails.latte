{**
 * Shows gallery image thumbnails.
 *
 * @param Nette\Database\Table\Selection $images
 *}

{if count($images) != 0}
	<ul class="GalleryThumbnails">
		{foreach $images as $image}
			<li>
				<a n:href="Gallery:showImage $image['Id']">
					<img src="{$baseUrl}/preview/{$image->ref('UploadedFileId')['Key']}/gallery"><span class="Name">{$image["Name"]}</span></a></li>
					
		{/foreach}
	</ul>
{else}
	<p class="ThisExpoIsEmpty">V této expozici nejsou žádné obrázky</p>
{/if}
